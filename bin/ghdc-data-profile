#!/usr/bin/env node

const yargs = require('yargs')

require('../profile')(
    require('yargs')
        .usage('$0 <data-dir>', 'profile collected ghdc-vuln data', yargs =>
            yargs.positional('data-dir', {
                describe: 'root data directory (contains commits/ directory)',
                type: 'string'
            })
        )
        .argv
)